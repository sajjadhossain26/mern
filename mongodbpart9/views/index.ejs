    <div class="container my-5">
        <div class="row">
            <div class="col-md-10 m-auto">
                <a class="btn btn-primary mb-2" href="students/create">Add new student</a>
                <div class="card">
                    <div class="card-header">
                        <h3>All Student</h3>
                    </div>
                    <div class="card-body">
                        <table class="table table-striped">
                           <thead>
                             <tr>
                                <th>Serial</th>
                                <th>Name</th>
                                <th>Cell</th>
                                <th>Email</th>
                                <th>Photo</th>
                                <th>Action</th>
                             </tr>
                           </thead>
                           <tbody>
                             <% student.forEach((element, index) => { %>
                              <tr>
                                <td><%= index +1 %> </td>
                                <td><%= element.name %> </td>
                                <td><%= element.cell %></td>
                                <td><%= element.email %></td>
                                <td>
                                    <img style="width: 50px;height: 50px; object-fit: cover" src="assets/upload/<%= element.photo %>" alt="">
                                </td>
                                <td>
                                    <a class="btn btn-primary btn-sm" href="/students/<%= element._id %> "><i class="fas fa-eye"></i></a>
                                    <a class="btn btn-info btn-sm" href="/students/edit/<%= element._id %> "><i class="fas fa-edit"></i></a>
                                    <a class="btn btn-danger btn-sm" href="/students/delete/<%= element._id %> "><i class="fas fa-trash"></i></a>
                                </td>
                             </tr>
                             <% }) %>
                           </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
       </div>

       